{
  "manifest_version": 3,
  "name": "Screen Time",
  "description": "Track usage of websites",
  "version": "1.0",
  "action": {
    "default_popup": "popup/popup.html"
  },
  "icons": {
    "64": "img/icon64.png"
    
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "storage"
  ]
}
